{
  "displayName": "GTMDatalayer",
  "name": "gtmdatalayer",
  "platform": "web",
  "version": "0.0.0",
  "description": "Use the same dataLayer as GTM, an array of objects instead of an object.",
  "author": {
    "name": "Lucas Kostka <lucas@fifty-five.com>"
  },
  "viewBasePath": "src/view/",
  "dataElements": [
    {
      "displayName": "dataLayer",
      "name": "datalayer",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/dataElements/datalayer.js"
    }
  ],
  "actions": [
    {
      "displayName": "dataLayer event",
      "name": "datalayer-event",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/actions/datalayerEvent.js"
    }
  ]
}
